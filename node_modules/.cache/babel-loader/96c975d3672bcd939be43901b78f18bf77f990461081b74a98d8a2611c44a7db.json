ndex(katalog => katalog.name === item.name)].sum++;\n      }\n    },\n    minus(name) {\n      let el = this.korzina.find(item => item.name == name);\n      if (el.sum != 1) {\n        el.sum--;\n      }\n    },\n    clear(name) {\n      this.korzina.splice(this.korzina.findIndex(katalog => katalog.name === name), 1);\n    },\n    ShowDetailed(name) {\n      this.hide(5);\n      this.detailedItem = this.katalog[this.katalog.findIndex(a => a.name == name)];\n    },\n    ShowLogin() {\n      this.login = !this.login;\n    },\n    ShowRegister() {\n      this.register = !this.register;\n    }\n  },\n  computed: {\n    reset() {\n      let amount = 0;\n      this.korzina.forEach(el => {\n        amount += el.sum * el.price;\n      });\n      return amount;\n    }\n  }\n};","map":{"version":3,"names":["tovarKatalog","buttonDef","tovarKorzina","useFetch","isFetching","error","data","json","name","components","vis","car_deral","login","register","katalog","opi","img","require","price","sale","weight","sum","main","korzina","detailedItem","slider","methods","hide","i","add","item","console","log","findIndex","push","minus","el","find","clear","splice","ShowDetailed","a","ShowLogin","ShowRegister","computed","reset","amount","forEach"],"sources":["C:\\OSPanel\\domains\\laba\\src\\App.vue"],"sourcesContent":["<template>\n  <header class=\"header\">\n    <div class=\"header_item\" @click=\"hide(3)\">\n      <svg xmlns=\"http://www.w3.org/2000/svg\" width=\"44\" height=\"44\" fill=\"#FBE5ED\" class=\"bi bi-house-heart\" viewBox=\"0 0 16 16\">\n        <path d=\"M8 6.982C9.664 5.309 13.825 8.236 8 12 2.175 8.236 6.336 5.309 8 6.982\"/>\n        <path d=\"M8.707 1.5a1 1 0 0 0-1.414 0L.646 8.146a.5.5 0 0 0 .708.707L2 8.207V13.5A1.5 1.5 0 0 0 3.5 15h9a1.5 1.5 0 0 0 1.5-1.5V8.207l.646.646a.5.5 0 0 0 .708-.707L13 5.793V2.5a.5.5 0 0 0-.5-.5h-1a.5.5 0 0 0-.5.5v1.293zM13 7.207V13.5a.5.5 0 0 1-.5.5h-9a.5.5 0 0 1-.5-.5V7.207l5-5z\"/>\n      </svg>\n      <p class=\"header_nav\">MAIN</p>\n