          if(this.vis!=4){\n            this.vis=4\n            this.car_deral=false\n          }\n          return;\n        }\n        case 5:{\n          if(!this.car_deral){\n            this.car_deral=!this.car_deral\n            this.vis=0\n          }\n          else{\n            this.car_deral=!this.car_deral\n            this.vis=1\n          }\n          return;\n        }\n      }\n    },\n    add(item){\n      console.log(item);\n      console.log(this.korzina.findIndex((katalog) => katalog.name === item.name));\n\n      if(this.korzina.findIndex((katalog) => katalog.name === item.name) === -1){\n        this.korzina.push({...item})\n        console.log(this.korzina);\n      }\n      else{\n        this.korzina[this.korzina.findIndex((katalog) => katalog.name === item.name)].sum++;\n      }\n    },\n    minus(name){\n      let el = this.korzina.find(item => item.name == name)\n      if(el.sum != 1){el.sum--}\n    },\n    clear(name){\n      this.korzina.splice(this.korzina.findIndex((katalog) => katalog.name === name), 1) \n    },\n    ShowDetailed(name){\n      this.hide(5)\n      this.detailedItem = this.katalog[this.katalog.findIndex(a => a.name == name)];\n    },\n    ShowLogin(){\n      this.login=!this.login\n    },\n    ShowRegister(){\n      this.register=!this.register\n    },\n  },\n  computed:\n  {\n    reset()\n    {\n      let amount = 0;\n      this.korzina.forEach((el) =>\n      {\n        amount += el.sum * el.price\n      })\n      return amount;\n    },\n  }\n}\n</script>\n\n<style>\n  /* ####ШРИФТЫ### */\n  @import url('https://fonts.googleapis.com/css2?family=Varela+Round&display=swap');\n  /* ####!ШРИФТЫ### */\n\n  /* ####НА ВСЕ### */\n  *{\n    margin: 0;\n    padding: 0;\n    border: 0;\n  }\n  body{\n    margin: 20px 100px;\n    background: #FBE5ED;\n    font-family: Varela Round;\n  }\n  header, main{\n    margin: auto;\n    width: 1280px;\n  }\n  /* ####!НА ВСЕ### */\n\n  /* ####ХЭДЕР### */\n  .header{\n    position: sticky;\n 