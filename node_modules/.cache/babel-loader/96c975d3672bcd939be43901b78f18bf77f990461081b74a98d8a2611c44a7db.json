img=\"el.img\"\n            :opi=\"el.opi\"\n            :price=\"el.price\"\n            :sale=\"el.sale\"\n            :sum=\"el.sum\"\n            :weight=\"el.weight\"\n            @plys = \"el.sum++\"\n            @minus = \"minus(el.name)\"\n            @clear = \"clear(el.name)\"\n          />\n        </div>\n        <div class=\"polno_card\">\n          <div class=\"polno_first\">\n            <p class=\"polno_card_text\">To pay!</p>\n            <div class=\"polno_card_flex\">\n              <p class=\"polno_card_flex_left\">Quantity:</p>\n              <p class=\"polno_card_flex_right\">{{ korzina.length }}</p>\n            </div>\n            <div class=\"polno_card_flex\">\n              <p class=\"polno_card_flex_left\">Total to be paid:</p>\n              <p class=\"polno_card_flex_right\">{{ reset }} $</p>\n            </div>\n          </div>\n          <button class=\"polno_card_but\">Buy</button>\n        </div>\n      </div>\n    </section>\n  </main>\n  <main class=\"twits\" v-show=\"vis==4\">\n    <section class=\"nologin\">\n      <img :src=\"require(`./assets/` + `nologin.png`)\" alt=\"\" class=\"nologin_img\"> \n      <button class=\"nologin_but\" @click=\"ShowLogin()\">Login / Register</button>\n    </section>\n    <section class=\"login\" v-show=\"login\">\n      <div class=\"modal-backdrop\">\n        <div class=\"modal\">\n          <form action=\"#\" method=\"get\" class=\"login_form\">\n            <svg @click=\"ShowLogin()\" class=\"close\" width=\"50\" height=\"50\" viewBox=\"0 0 50 50\" fill=\"none\" xmlns=\"http://www.w3.org/2000/svg\">\n              <circle cx=\"24.88\" cy=\"24.7887\" r=\"23.4\" fill=\"#FF77A4\"/>\n              <rect x=\"11.6431\" y=\"36.1348\" width=\"34.7657\" height=\"2.67429\" rx=\"1.33714\" transform=\"rotate(-45 11.6431 36.1348)\" fill=\"#D9D9D9\"/>\n              <rect x=\"36.2261\" y=\"38.0258\" width=\"34.7657\" height=\"2.67429\" rx=\"1.33714\" transform=\"rotate(-135 36.2261 38.0258)\" fill=\"#D9D9D9\